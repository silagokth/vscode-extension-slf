{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "VS-CSTR",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#keywords-operator"
    },
    {
      "include": "#labels"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line",
          "match": "#.*$"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "markup.bold",
          "match": "\\b(epoch|type)\\b"
        }
      ]
    },
    "keywords-operator": {
      "patterns": [
        {
          "name": "keyword.control",
          "match": "\\b(linear)\\b"
        }
      ]
    },
    "labels": {
      "patterns": [
        {
          "name": "entity.name.tag",
          "match": "<[^>]*>"
        }
      ]
    }
  },
  "scopeName": "source.cstr"
}
